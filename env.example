# Redshift connection settings
# Copy this file to .env and fill in your actual values

# Redshift cluster endpoint
RS_HOST=your-redshift-cluster.region.redshift.amazonaws.com

# Redshift port (default is 5439)
RS_PORT=5439

# Redshift username
RS_USER=your_username

# Redshift password
RS_PASSWORD=your_password

# Redshift database name
RS_DATABASE=your_database

# Redshift schema (optional, defaults to "public")
RS_SCHEMA=public